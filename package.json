{
  "name": "sites",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node dist/src/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "lint:check": "eslint . --ext .ts",
    "test": "jest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node -r ts-node/register src/db/migrate.ts",
    "db:migrate:fix": "node -r ts-node/register src/db/migrate-fix.ts",
    "db:migrate:manual": "node -r ts-node/register src/db/manual-migrate.ts",
    "db:migrate:manual:js": "node src/db/manual-migrate.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "amqp-connection-manager": "^4.1.15",
    "amqplib": "^0.10.8",
    "archiver": "^7.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cron": "^3.1.7",
    "drizzle-orm": "^0.45.1",
    "minio": "^7.1.3",
    "nestjs-pino": "^4.5.0",
    "pg": "^8.12.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
